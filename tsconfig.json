{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es2015"
        ],
        "sourceMap": true,
        "outDir": "lib",
        "rootDir": "src",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "exclude": [
        "lib",
        "e2e",
        "node_modules",
        "test-workspace"
    ]
}